<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000">

<Button
        android:id="@+id/test"
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:visibility="gone" />

<ViewFlipper
 android:id="@+id/splash_flipper"
 android:layout_width="fill_parent"
 android:layout_height="fill_parent">

<ImageView
    android:id="@+id/splash_dope"
    android:src="@drawable/dope" 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
/>

<ImageView
    android:id="@+id/splash_boys"
    android:src="@drawable/boys" 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
/>

<ImageView
    android:id="@+id/splash_and"
    android:src="@drawable/and" 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
/>

<ImageView
    android:id="@+id/splash_girls"
    android:src="@drawable/girls" 
    android:layout_width="match_parent"
    android:layout_height="match_parent"

/>



<ImageView
    android:id="@+id/splash_kt"
    android:src="@drawable/kt_logo" 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
/>



</ViewFlipper>



<ImageView
    android:layout_width="25dp"
    android:layout_height="25dp"
    android:src="@draw"
/>

<ImageView
    android:layout_width="25dp"
    android:layout_height="25dp"
/>

<ImageView
    android:layout_width="25dp"
    android:layout_height="25dp"
/>

</RelativeLayout>

